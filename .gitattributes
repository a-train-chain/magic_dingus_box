# Media
*.mp4 filter=lfs diff=lfs merge=lfs -text
*.mp3 filter=lfs diff=lfs merge=lfs -text
*.flac filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text
# ROMs / disc images
*.nes filter=lfs diff=lfs merge=lfs -text
*.sfc filter=lfs diff=lfs merge=lfs -text
*.smc filter=lfs diff=lfs merge=lfs -text
*.gb filter=lfs diff=lfs merge=lfs -text
*.gbc filter=lfs diff=lfs merge=lfs -text
*.gba filter=lfs diff=lfs merge=lfs -text
*.n64 filter=lfs diff=lfs merge=lfs -text
*.z64 filter=lfs diff=lfs merge=lfs -text
*.v64 filter=lfs diff=lfs merge=lfs -text
*.bin filter=lfs diff=lfs merge=lfs -text
*.cue filter=lfs diff=lfs merge=lfs -text
*.iso filter=lfs diff=lfs merge=lfs -text
*.chd filter=lfs diff=lfs merge=lfs -text
*.ecm filter=lfs diff=lfs merge=lfs -text
